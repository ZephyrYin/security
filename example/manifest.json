{
  "manifest_version": 2,

  "name": "Getting started example",
  "description": "This extension shows a Google Image search result for the current page",
  "version": "1.01",

  "browser_action": {
    "default_icon": "./imgs/logo.png",
    "default_title": "Leak Sentry"
  },
  
  "background": {
    "page": "background.html",
    "persistent": false
  },
  
  "permissions": [
    "activeTab",
    "http://*/",
	"https://*/"
  ]
  
}
